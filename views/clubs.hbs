

<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Location</th>
    </tr>
  </thead>
  <tbody>
    {{#each clubs.clubs}} 
    <tr>
      <th scope="row">{{@index}}</th>
      <td>{{_id}}</td>
      <td>{{clubName}}</td>
      <td>{{clubLocation}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
  
<form action="api/clubs" method="POST">
    <div class="form-group">
        <label for="club name">Club Name</label>
        <input type="text" class="form-control" name="clubName" placeholder="Enter the name of the club">
    </div>
    <div class="form-group">
        <label for="club location">Club Location</label>
        <input type="text" class="form-control" name="clubLocation" placeholder="Enter the location of the club">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>