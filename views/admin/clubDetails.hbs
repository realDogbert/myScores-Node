<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/admin">Administration</a></li>
    <li class="breadcrumb-item active"><a href="/admin/clubs">Club Management</a></li>
    <li class="breadcrumb-item active"  aria-current="page">{{club.clubName}}</li>
  </ol>
</nav>

<h1>Club Details</h1>

<form>

    <input type="text" value="{{club._id}}" hidden>
    <div class="form-group row">
        <label for="clubName">Club Name</label>
        <input class="form-control" type="text" value="{{club.clubName}}">
    </div>
    <div class="form-group row">
        <label for="clubLocation">Club Location</label>
        <input class="form-control" type="text" value="{{club.clubLocation}}">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
    <button type="button" class="btn btn-secondary" id="cancel">Cancel</button>

</form>

