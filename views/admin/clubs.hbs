<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/admin">Administration</a></li>
    <li class="breadcrumb-item active" aria-current="page">Club Management</li>
  </ol>
</nav>

<h1>
  Club
  <button type="button" class="btn btn-primary" id="clubbNew">+</button>
</h1>


<table class="table table-sm table-striped table-hover">
  <thead>
    <tr>
      <th>Club Name</th>
      <th>Club Location</th>
    </tr>
  </thead>
  <tbody>
    {{#each clubs}}
      <tr id="{{_id}}">
        <td>
          <a href="/admin/clubs/{{_id}}">{{name}}</a>
        </td>
        <td>{{location}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>


