
<h1>Clubs</h1>
<p>Welcome {{user.name}}</p>
  
<form action="/api/clubs" method="POST">
    <div class="form-group">
        <label for="club name">Club Name</label>
        <input type="text" class="form-control" name="clubName" placeholder="Enter the name of the club">
    </div>
    <div class="form-group">
        <label for="club location">Club Location</label>
        <input type="text" class="form-control" name="clubLocation" placeholder="Enter the location of the club">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>

</form>

<div id="test"></div>

<script src="javascripts/courseForm.js"></script>