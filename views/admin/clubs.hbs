<h1>Course</h1>

  
<form>

    <div class="form-group">
      <label for="clubselector">Club Name</label>
      <select class="form-control" id="clubselector" name="club[id]">
        <option selected>Please select</option>
        {{#each clubs}} 
          <option value="{{_id}}">{{clubName}} {{clubLocation}}</option>
        {{/each}}
      </select>
    </div>
    
</form>



